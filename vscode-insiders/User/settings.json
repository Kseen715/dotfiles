{
    "workbench.colorTheme": "Gruvbox Dark Medium",
    "editor.rulers": [
        {
            "column": 80,
            "color": "#616161"
        }
    ],
    "git.confirmSync": false,
    "git.autofetch": true,
    "git.enableSmartCommit": true,
    "git.pruneOnFetch": true,
    "git.useCommitInputAsStashMessage": true,
    "github.copilot.editor.enableAutoCompletions": true,
    "github.copilot.enable": {
        "*": true,
        "plaintext": false,
        "markdown": true,
        "scminput": false
    },
    "git-graph.repository.commits.fetchAvatars": true,
    "git-graph.repository.commits.mute.commitsThatAreNotAncestorsOfHead": true,
    "git-graph.integratedTerminalShell": "C:\\Program Files\\PowerShell\\7\\pwsh.exe",
    "gitlens.graph.minimap.enabled": false,
    "githubPullRequests.createOnPublishBranch": "never",
    "githubPullRequests.pullBranch": "never",
    "githubPullRequests.pushBranch": "always",
    "files.autoSave": "afterDelay",
    "editor.formatOnSaveMode": "modifications",
    "security.allowedUNCHosts": [
        "wsl.localhost"
    ],
    "security.workspace.trust.untrustedFiles": "open",
    "cmake.showOptionsMovedNotification": false,
    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false,
    "explorer.confirmPasteNative": false,
    "terminal.external.windowsExec": "C:\\Program Files\\PowerShell\\7\\pwsh.exe",
    "terminal.integrated.bellDuration": 500,
    "terminal.integrated.confirmOnKill": "never",
    "terminal.integrated.cursorStyle": "line",
    "translation.target-language": "ru",
    "C_Cpp.formatting": "vcFormat",
    "[yaml]": {
        "editor.defaultFormatter": "redhat.vscode-yaml"
    },
    "[vue]": {
        "editor.defaultFormatter": "Vue.volar"
    },
    "makefile.configureOnOpen": false,
    "vsicons.dontShowNewVersionMessage": true,
    "liveServer.settings.donotShowInfoMsg": true,
    "chat.editor.wordWrap": "on",
    "editor.fontFamily": "Cascadia Code",
    "editor.wordWrap": "wordWrapColumn",
    "editor.minimap.maxColumn": 80,
    "editor.acceptSuggestionOnEnter": "smart",
    "editor.inlineSuggest.fontFamily": "Iosevka Term",
    "editor.inlineSuggest.syntaxHighlightingEnabled": true,
    "scm.alwaysShowRepositories": true,
    "scm.defaultViewMode": "tree",
    "translation.voice-api-provider": "Microsoft",
    "accessibility.voice.speechLanguage": "ru-RU",
    "workbench.startupEditor": "none",
    "update.showReleaseNotes": false,
    "window.zoomLevel": 0,
    "background.enabled": true,
    "background.editor": {
        "useFront": true,
        "style": {
            "content": "''",
            "position": "center",
            "width": "100%",
            "height": "100%",
            "repeat": "no-repeat",
            "z-index": 1,
            "background-position": "100% 100%",
            "background-size": "50%",
            "opacity": 0.1
        },
        "styles": [
            {},
            {},
            {}
        ],
        "images": [
            "file:///D:/Pictures/Kokkoro-astrum1-sprite-normal.png",
        ],
        "interval": 0,
        "random": false
    },
    "background.customImages": [
        "file:///D:/Pictures/Kokkoro-astrum1-sprite-normal.png"
    ],
    "background.style": {
        "content": "''",
        "position": "center",
        "width": "100%",
        "height": "100%",
        "repeat": "no-repeat",
        "z-index": 1,
        "background-position": "100% 100%",
        "background-size": "50%",
        "opacity": 0.1,
    },
}